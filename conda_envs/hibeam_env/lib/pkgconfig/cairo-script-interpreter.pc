prefix=/projects/hep/fs10/shared/nnbar/bmeirose/Billys_GGN/GGN_scripts/conda_envs/hibeam_env
includedir=${prefix}/include
libdir=${prefix}/lib

Name: cairo-script-interpreter
Description: script surface backend for cairo graphics library
Version: 1.18.4
Requires: cairo
Requires.private: zlib, libpng >=  1.4.0, fontconfig >=  2.13.0, freetype2 >=  23.0.17, x11, xext, xrender >=  0.6, xcb >=  1.6, xcb-render >=  1.6, x11-xcb, xcb-shm, pixman-1 >=  0.40.0
Libs: -L${libdir} -lcairo-script-interpreter
Libs.private: -lm -ldl
Cflags: -I${includedir}/cairo
